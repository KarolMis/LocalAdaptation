     CREATE OR REPLACE FUNCTION GET_ATR
     (OBJNAME VARCHAR(15))
     RETURNS VARCHAR(20)
     LANGUAGE SQL
     READS SQL DATA
     NO EXTERNAL ACTION

     BEGIN

      DECLARE ATRIBUT VARCHAR(20) NOT NULL DEFAULT '';

      FOR CURSOR AS C1 CURSOR
          FOR SELECT CATR FROM DISCOBJ WHERE NAME = OBJNAME
      DO SET ATRIBUT =  RTRIM(CURSOR.CATR) CONCAT ', ' CONCAT ATRIBUT ;
      END FOR;

      RETURN ATRIBUT  ;
     END
